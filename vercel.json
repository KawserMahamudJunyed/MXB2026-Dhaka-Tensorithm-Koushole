{
    "version": 2,
    "routes": [
        {
            "src": "/reset-password",
            "dest": "/public/reset-password.html"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/admin",
            "destination": "/public/admin.html"
        },
        {
            "source": "/reset-password",
            "destination": "/public/reset-password.html"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ]
}